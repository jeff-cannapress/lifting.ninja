{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://lifting.ninja/schemas/measurementItem.schema.json",
    "title": "Entity",
    "description": "An measured item",
    "type": "object",
    "properties":{
        "measure":{
            "enum":["Weight","Reps","Time","Distance"]
        },
        "units":{
            "type":"string"
        }
    },
    "required":["measure","units"]
}